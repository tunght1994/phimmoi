{
    "extends": [
      "tslint-react",
      "tslint-config-airbnb"
    ],
    "rules": {
      "import-name": false,
      "variable-name": false,
      "jsx-no-lambda": false,
      "indent": [true, 4],
      "ter-indent": [true, 4],
      "no-boolean-literal-compare": true
    },
    "plugins": [
      "react"
    ]
}
